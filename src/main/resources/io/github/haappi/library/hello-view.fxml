      <Text fx:id="personInformation" layoutX="90.0" layoutY="339.0" strokeType="OUTSIDE" strokeWidth="0.0"
            text="your's Information" textAlignment="CENTER" visible="false"/>
      <Button fx:id="higherLimitButton" layoutX="48.0" layoutY="520.0" mnemonicParsing="false" onAction="#higherLimit"
              text="Request Higher Checkout Limit" visible="false"/>
   </children>
</AnchorPane>
