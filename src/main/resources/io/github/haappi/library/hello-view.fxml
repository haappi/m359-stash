      <TextField fx:id="input" layoutX="327.0" layoutY="388.0" onAction="#inputButtonSlapped"/>
      <Text layoutX="327.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Library">
         <font>
            <Font size="41.0"/>
         </font>
      </Text>
      <Text fx:id="bookInfo" layoutX="610.0" layoutY="381.0" strokeType="OUTSIDE" strokeWidth="0.0"
            text="Book Information"/>
