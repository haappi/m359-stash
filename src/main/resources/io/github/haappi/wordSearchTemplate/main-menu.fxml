<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18"
            fx:controller="io.github.haappi.wordSearchTemplate.MainMenuController">
    <children>
        <Button layoutX="260.0" layoutY="88.0" mnemonicParsing="false" onAction="#actualGame" text="Go to Game"/>
        <Button layoutY="2.0" mnemonicParsing="false" onAction="#exitGame" text="Main Menu" textFill="#e40505"
                underline="true">
            <font>
                <Font size="26.0"/>
            </font>
        </Button>
        <ListView fx:id="highScores" layoutX="200.0" layoutY="159.0" prefHeight="200.0" prefWidth="200.0"/>
        <Button layoutX="408.0" layoutY="247.0" mnemonicParsing="false" onAction="#refreshLeader"
                text="Rrefresh Leaderboard"/>
    </children>
</AnchorPane>
