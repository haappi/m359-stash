<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18"
            fx:controller="io.github.haappi.battleGame.EquipItems">
    <children>
        <ListView fx:id="inventoryItems" layoutX="31.0" layoutY="119.0" onMouseClicked="#onItemAdded" prefHeight="189.0"
                  prefWidth="178.0"/>
        <Text fill="#3bc4eb" layoutX="55.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="All weapons will be available during the battle.">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <ListView fx:id="selectedItems" layoutX="258.0" layoutY="119.0" onMouseClicked="#onItemRemoved"
                  prefHeight="189.0" prefWidth="178.0"/>
        <Text layoutX="300.0" layoutY="99.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Selected Items"/>
        <Button layoutX="475.0" layoutY="188.0" mnemonicParsing="false" text="Done!"/>
        <Text layoutX="79.0" layoutY="99.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Inventory Items"/>
    </children>
</AnchorPane>
