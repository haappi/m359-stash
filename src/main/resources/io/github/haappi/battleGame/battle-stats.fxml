<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18"
            fx:controller="io.github.haappi.battleGame.BattleStats">
    <children>
        <LineChart fx:id="lineChart" layoutX="28.0" visible="false">
            <xAxis>
                <CategoryAxis side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT"/>
            </yAxis>
        </LineChart>
        <Button layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#mainMenu" text="Main Menu"/>
        <Text fx:id="wins" layoutX="70.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wins / Losses"/>
        <Text fx:id="dmgDealt" layoutX="198.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="AVG Damage Dealt"/>
        <Text fx:id="dmgTaken" layoutX="385.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="AVG Damage Taken"/>
    </children>
</AnchorPane>
